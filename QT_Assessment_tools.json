        {
          "name": "job_id",
          "type": "string",
          "description": "ID of the job"
        },
        {
          "name": "context",
          "type": "object",
          "description": "Context information for the agent",
          "required": false
        }
      ],
      "return": {
        "type": "object",
        "description": "Result of the agent invocation"
      }
