{
  "available_tools": [
    {
      "name": "UpdateAgentInstance",
      "description": "Updates the status of an agent instance in the platform",
      "resource": "arn:aws:lambda:us-west-2:411097365838:function:ElasticGumbyUniversalOrchAgentPrototype-DemoLambda",
      "parameters": [
        {
          "name": "agent_id",
          "type": "string",
          "description": "ID of the agent instance to update"
        },
        {
          "name": "status",
          "type": "string",
          "description": "New status of the agent (e.g., 'RUNNING', 'COMPLETED', 'FAILED')"
        }
      ],
      "return": {
        "type": "object",
        "description": "Result of the update operation"
      }
    },
    {
      "name": "GetJob",
      "description": "Retrieves job information from the platform",
      "resource": "arn:aws:lambda:us-west-2:411097365838:function:ElasticGumbyUniversalOrchAgentPrototype-DemoLambda",
      "parameters": [
        {
          "name": "job_id",
          "type": "string",
          "description": "ID of the job to retrieve"
        }
      ],
      "return": {
        "type": "object",
        "description": "Details of the requested job"
      }
    },
    {
      "name": "UpdateJob",
      "description": "Updates job information in the platform",
      "resource": "arn:aws:lambda:us-west-2:411097365838:function:ElasticGumbyUniversalOrchAgentPrototype-DemoLambda",
      "parameters": [
        {
          "name": "job_id",
          "type": "string",
          "description": "ID of the job to update"
        },
        {
          "name": "status",
          "type": "string",
          "description": "New status of the job",
          "required": false
        },
        {
          "name": "context",
          "type": "object",
          "description": "Additional context information for the job",
          "required": false
        }
      ],
      "return": {
        "type": "object",
        "description": "Result of the update operation"
      }
    },
    {
